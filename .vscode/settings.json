{
  "files.associations": {
    "*.json5": "jsonc",
    "**/ansible/**/*.yml": "ansible",
    "**/ansible/**/*.sops.yml": "yaml",
    "**/ansible/**/inventory/**/*.yml": "yaml",
    "**/terraform/**/*.tf": "terraform",
    "**/cluster/**/*.sops.toml": "plaintext"
  },
  "yaml.schemas": {
    "ansible": "ansible/*.yml",
    "Kubernetes": "cluster/*.yaml",
    "https://kubernetes-schemas.devbu.io/helm.toolkit.fluxcd.io/helmrelease_v2beta1.json": "file:///c%3A/Users/Mark%20Mckessock/Documents/kube-saturn/cluster/apps/security/external-secrets/stores/onepassword/helmrelease.yaml"
  },
  "yaml.customTags": [
    "!secret scalar",
    "!env_var scalar"
  ],
  "prettier.configPath": ".ci/prettier/.prettierrc.yaml",
  "prettier.ignorePath": ".ci/prettier/.prettierignore",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.hover.delay": 1500,
  "files.trimTrailingWhitespace": true
}
